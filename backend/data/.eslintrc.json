{
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es2020": true
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-undef": "error",
        "no-console": "warn",
        "eqeqeq": "error"
    }
}
